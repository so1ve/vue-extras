{
  "private": true,
  "packageManager": "pnpm@10.18.0",
  "type": "module",
  "scripts": {
    "build": "pnpm -r run build",
    "lint": "eslint . || prettier . --check",
    "lint:fix": "eslint . --fix || prettier . --write",
    "prepublishOnly": "nr build",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm publish -r --filter ./packages/*",
    "test": "vitest",
    "typecheck": "tsgo --noEmit",
    "watch": "pnpm -r --parallel run watch"
  },
  "devDependencies": {
    "@antfu/ni": "^25.0.0",
    "@so1ve/eslint-config": "^3.9.0",
    "@so1ve/prettier-config": "^3.9.0",
    "@types/node": "^24.3.1 ",
    "@typescript/native-preview": "7.0.0-dev.20251005.1",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
